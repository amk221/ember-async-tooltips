{{#each tooltippers as |tooltipper|}}
  {{component
    tooltipper.tooltip
    data=tooltipper.data
    _tooltipper=tooltipper
    -on-insert=(action "tooltipInserted" target=tooltipper)
    -on-mouse-leave=(action "tooltipExited" target=tooltipper)
    -on-hide=(action "tooltipHidden" target=tooltipper)
  }}
{{/each}}