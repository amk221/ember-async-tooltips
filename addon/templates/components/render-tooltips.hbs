{{#each this.tooltipService.tooltippers as |tooltipper|}}
  {{component
    tooltipper.tooltip
    data=tooltipper.data
    error=tooltipper.error
    isLoaded=tooltipper.isLoaded
    tooltipperInstance=tooltipper
    _onInsert=(action "tooltipInserted" target=tooltipper)
    _onMouseLeave=(action "tooltipExited" target=tooltipper)
    _onHide=(action "tooltipHidden" target=tooltipper)
  }}
{{/each}}